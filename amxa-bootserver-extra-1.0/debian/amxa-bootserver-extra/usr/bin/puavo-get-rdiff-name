#!/bin/sh

PREV=$1
N=$2


     PRE=$(echo $PREV|cut -d- -f1-3)
#     POST=$(echo $PREV|cut -d- -f8)
     POST="i386.rdiff"
     FIRST=$(echo $PREV|cut -d- -f4-7)
     SECOND=$(echo $N|cut -d- -f4-7)
     RDIFF="${PRE}-${FIRST}--${SECOND}-${POST}"
#    RDIFF=$(get_rdiff_filename $PREV $N)
     echo $RDIFF

exit

